Lauching Strix with Parameters:  -f (
 !a &
 !b &
 !c &
 !hbusreq_1 &
 !hbusreq_2 &
 !allready &
 !hbusreq_0 &
 X G (a | b | c | hbusreq_1 | hbusreq_2 | allready | hbusreq_0 ) &
 X G ((!b & !c & !hbusreq_1 & !hbusreq_2 & !allready & !hbusreq_0 ) | (!a & !c & !hbusreq_1 & !hbusreq_2 & !allready & !hbusreq_0 ) | (!a & !b & !hbusreq_1 & !hbusreq_2 & !allready & !hbusreq_0 ) | (!a & !b & !c & !hbusreq_2 & !allready & !hbusreq_0 ) | (!a & !b & !c & !hbusreq_1 & !allready & !hbusreq_0 ) | (!a & !b & !c & !hbusreq_1 & !hbusreq_2 & !hbusreq_0 ) | (!a & !b & !c & !hbusreq_1 & !hbusreq_2 & !allready )  ) &
 X (G F allready) &
 X (allready) ) -> ( !p0 &
 !p1 &
 !p2 &
 !hgrant_2 &
 !hgrant_1 &
 !decide &
 !hgrant_0 &
 !busreq &
 X (G ((p0 & !p1 & !p2) | (!p0 &  p1 & !p2) | (!p0 & !p1 & p2))) &
 X ((F G a | G F b) <-> (G F p0 | (G F p2 & !G F p1))) &
 X (G ((!hgrant_0 & ! hgrant_1) | ((!hgrant_0 | ! hgrant_1) & !hgrant_2))) &
 X (G (hgrant_0 | hgrant_1 | hgrant_2)) &
 X (G (!allready -> (X hgrant_0 <-> hgrant_0))) &
 X (G (!allready -> (X hgrant_1 <-> hgrant_1))) &
 X (G (!allready -> (X hgrant_2 <-> hgrant_2))) &
 X (G (hbusreq_0 -> F (!hbusreq_0 | hgrant_0))) &
 X (G (hbusreq_1 -> F (!hbusreq_1 | hgrant_1))) &
 X (G (hbusreq_2 -> F (!hbusreq_2 | hgrant_2))) &
 X (G (hgrant_0 -> (busreq <-> hbusreq_0))) &
 X (G (hgrant_1 -> (busreq <-> hbusreq_1))) &
 X (G (hgrant_2 -> (busreq <-> hbusreq_2))) &
 X (G (!allready -> !decide)) &
 X (G (decide <-> (!(X hgrant_0 <-> hgrant_0) | !(X hgrant_1 <-> (hgrant_1)) | !(X hgrant_2 <-> (hgrant_2))))) &
 X (G ((!hbusreq_0 & !hbusreq_1 & !hbusreq_2 & decide) -> X hgrant_0)) &
 X (! decide) &
 X (hgrant_0) ) --ins=a,b,c,allready,hbusreq_0,hbusreq_1,hbusreq_2 --outs=p0,p1,p2,decide,busreq,hgrant_0,hgrant_1,hgrant_2 --k
